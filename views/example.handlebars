<div class="container">
  <div class="row">
    <div class="col-8 offset-2">
      <div class="card mt-4">
        <div class="card-body">
          <h5 class="card-title">Example</h5>

          {{#each voyager}}


          <div class="card-content blue-text">
            <p class='white-text'>Trip Details</p>
            <br>
            <p class='blue-text'>
              Astronaut Name: {{this.astronaut}}
              <br>
              Space Craft: {{this.space_craft}}
              <br>
              Planet: {{this.planet}}
              <br>
              Return Trip: {{this.return_trip}}
              <br>
              <br>
              <br>
              <hr>
            </p>
          </div>

          {{/each}}

        </div>
      </div>
    </div>
  </div>
</div>